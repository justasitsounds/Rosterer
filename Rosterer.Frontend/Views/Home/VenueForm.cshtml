@using Rosterer.Frontend.Models
@model Rosterer.Frontend.Models.VenueModel

<h3>Venue</h3>
@using (Ajax.BeginForm("Venue", new AjaxOptions() { OnSuccess = "CloseVenueModal", InsertionMode = InsertionMode.Replace, HttpMethod = "Post" }))
{
    <div class="editor-label">
        @Html.LabelFor(m => m.Name)
    </div>
    <div class="editor-field">
        @Html.TextBoxFor(m => m.Name)
        @Html.ValidationMessageFor(m => m.Name)
    </div>
    <div class="editor-label">
        @Html.LabelFor(m => m.Address1)
    </div>
    <div class="editor-field">
        @Html.TextBoxFor(m => m.Address1)
        @Html.ValidationMessageFor(m => m.Address1)
    </div>
    <div class="editor-label">
        @Html.LabelFor(m => m.Address2)
    </div>
    <div class="editor-field">
        @Html.TextBoxFor(m => m.Address2)
        @Html.ValidationMessageFor(m => m.Address2)
    </div>
    <div class="editor-label">
        @Html.LabelFor(m => m.State)
    </div>
    <div class="editor-field">
        @Html.DropDownListFor(m => m.State,new SelectList(Enum.GetNames(typeof(State))))
    </div>
    <div class="editor-label">
        @Html.LabelFor(m => m.Notes)
    </div>
    <div class="editor-field">
        @Html.TextAreaFor(m => m.Notes,5,30,new{})
    </div>
    <input type='button' id="closeVenueForm" value="Cancel"/>    <input type="submit" value="New Venue"/>
}
<script type="text/javascript">
    var completeForm = function () {
        window.alert("oi");
    }

</script>